[core]
	editor = nvim
  autocrlf = false

[mergetool "fugitive"]
	cmd = nvim  -c \"Gdiff\" \"$MERGED\"
[merge]
	tool = fugitive
[mergetool]
	keepBackup = false
[diff]
    tool = default-difftool
[difftool "default-difftool"]
  cmd = code --wait --diff $LOCAL $REMOTE

[pull]

[push]
  autoSetupRemote = true

